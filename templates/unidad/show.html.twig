{% extends 'base.html.twig' %}

{% block title %}Unidad{% endblock %}

{% block body %}
    <h1>Unidad</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ unidad.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ unidad.nombre }}</td>
            </tr>
            <tr>
                <th>Codigo</th>
                <td>{{ unidad.codigo }}</td>
            </tr>
            <tr>
                <th>Habilitada</th>
                <td>{{ unidad.habilitada ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('unidad_index') }}">back to list</a>

    <a href="{{ path('unidad_edit', {'id': unidad.id}) }}">edit</a>

    {{ include('unidad/_delete_form.html.twig') }}
{% endblock %}
